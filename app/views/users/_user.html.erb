<div id="<%= dom_id user %>">
  <p>
    <strong>Name:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Phone number:</strong>
    <%= user.phone_number %>
  </p>

  <p>
    <strong>Password digest:</strong>
    <%= user.password_digest %>
  </p>

  <p>
    <strong>Code:</strong>
    <%= user.code %>
  </p>

  <p>
    <strong>Verified:</strong>
    <%= user.verified %>
  </p>

</div>
